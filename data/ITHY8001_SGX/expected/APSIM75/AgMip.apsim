            LAIMax = Math.Max(LAIMax, LAI);
      SumEP = SumEP + ep;
      SumNO3Uptake = SumNO3Uptake + no3_uptake;
      InCropRain = InCropRain + Rain;
      SumLeachNO3 = SumLeachNO3 + flow_no3[flow_no3.Length - 1];
      
      
